{
    "name": "@sonoma-labs/cli",
    "version": "0.1.0",
    "description": "Command-line interface for Sonoma Labs Toolkit",
    "main": "dist/index.js",
    "bin": {
      "sonoma": "./bin/sonoma.js"
    },
    "scripts": {
      "build": "tsc",
      "watch": "tsc -w",
      "test": "jest",
      "lint": "eslint src --ext .ts",
      "format": "prettier --write \"src/**/*.ts\"",
      "clean": "rm -rf dist",
      "prepare": "npm run build",
      "start": "node ./bin/sonoma.js"
    },
    "keywords": [
      "solana",
      "blockchain",
      "cli",
      "autonomous-agents",
      "web3",
      "crypto",
      "defi"
    ],
    "author": "Sonoma Labs Team",
    "license": "MIT",
    "dependencies": {
      "@sonoma-labs/toolkit": "^0.1.0",
      "commander": "^11.1.0",
      "inquirer": "^8.2.6",
      "chalk": "^4.1.2",
      "ora": "^5.4.1",
      "conf": "^10.2.0",
      "update-notifier": "^5.1.0",
      "figlet": "^1.7.0",
      "dotenv": "^16.3.1",
      "@solana/web3.js": "^1.87.0",
      "axios": "^1.6.2",
      "winston": "^3.11.0"
    },
    "devDependencies": {
      "@types/inquirer": "^8.2.10",
      "@types/jest": "^29.5.10",
      "@types/node": "^20.10.0",
      "@types/figlet": "^1.5.8",
      "@types/update-notifier": "^5.1.0",
      "@typescript-eslint/eslint-plugin": "^6.13.0",
      "@typescript-eslint/parser": "^6.13.0",
      "eslint": "^8.54.0",
      "jest": "^29.7.0",
      "prettier": "^3.1.0",
      "ts-jest": "^29.1.1",
      "typescript": "^5.3.2"
    },
    "engines": {
      "node": ">=16.0.0"
    },
    "files": [
      "dist",
      "bin"
    ],
    "publishConfig": {
      "access": "public"
    }
  }